# -*- text -*-
# test task input
begin model set
  output "set.out"
  begin integrator kmc
    scale 1.0e14
    begin state
      begin quantity
	p[A] = 1.0e5		# Pa
	p[B] = 1.0e4		# Pa
	p[C] = 1.0e4		# Pa
      end quantity
      begin output
	(1.0e0 1.0e1)
      end output
      begin reactor batch
	temperature 3.0e2	# K
	pressure 1.2e5		# Pa
	volume 1.0e-5		# m^3
	rate_numerator moles
	rate_denominator volume
	fluid_quantity pressure
      end reactor
    end state
  end integrator
end model
